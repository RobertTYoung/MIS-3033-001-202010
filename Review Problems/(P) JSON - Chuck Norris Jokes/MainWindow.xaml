<Window x:Class="_P__JSON___Chuck_Norris_Jokes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_P__JSON___Chuck_Norris_Jokes"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="https://images02.military.com/sites/default/files/styles/full/public/media/veteran-jobs/content-images/2016/03/chucknorris.jpg?itok=_J3M4O-S"/>
        </Grid.Background>
        <ComboBox x:Name="cboCategory" HorizontalAlignment="Left" Height="31" Margin="26,10,0,0" VerticalAlignment="Top" Width="149"/>
        <Button x:Name="btnGetQuote" Content="Get Quote" HorizontalAlignment="Left" Height="31" Margin="180,10,0,0" VerticalAlignment="Top" Width="124" Click="btnGetQuote_Click"/>
        <RichTextBox x:Name="rtbQuote" HorizontalAlignment="Left" Height="177" Margin="526,10,0,0" VerticalAlignment="Top" Width="258">
            <FlowDocument>
                <Paragraph>
                    <Run Text=""/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>

    </Grid>
</Window>
